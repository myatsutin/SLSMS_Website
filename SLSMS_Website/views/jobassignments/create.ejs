     <div class="main-panel">
            
            <div class="container-fluid">
                <div class="row">
                    <div class="col-md-12">
                        <div class="card">
                            <div class="header">
            <h5><a href="/jobassignments">Job Assignment Tracker</a></h5>
			</div>
			
						  		   
<input type="button" class="btn btn-info" value="Add New Job" onclick="location.href = 'http://localhost:3000/jobs/create';">
	
			<div class = "content">		   
            <form action="/jobassignments/save" method="post">
          	
							<div class="row">
                                <div class="col-md-4">
								     <div class="form-group">
                                           <label>Auditor *</label>
                                                <select id ="inputAuditior" class ="form-control">
													<option selected>---Select---</option>
													<option>Mariah</option>
													<option>Nitin Gandhi</option>
												</select>
											</div>
                                      </div>
									  
								<div class ="col-md-4">
									<div class = "form-group">
										  <label>Job Type *</label>
                                                <select id ="inputJobType" class ="form-control">
													<option selected>---Select---</option>
													<option>Live Navigation Audit</option>
												</select>
									</div>
								</div>
							</div>
									  
							<div class="row">
                                <div class="col-md-4">
								     <div class="form-group">
                                           <label>Vessel *</label>
                                                <select id ="inputVessel" class ="form-control">
													<option selected>---Select---</option>
													<option>Unique Infinity</option>
												</select>
									</div>
                                </div>
								
								<div class="col-md-4">
								    <div class="form-group">
									<label>Company *</label>
                                                <select id ="inputCompany" class ="form-control">
													<option selected>---Select---</option>
													<option>AESM</option>
												</select>
									</div>
								</div>
								
							</div>
                                
							<div class="row">
								<div class = "col-md-4">
									<div class="form-group">
										<label>Assigned Date *</label>
											  <input type="date" class="form-control" placeholder="Assigned Date" name="dateofassigned" required>
									</div>
								</div>
								
								<div class = "col-md-4">
									<div class="form-group">
										<label>Travel Date *</label>
											  <input type="date" class="form-control" placeholder="Travel Date" name="dateoftravel" required>
									</div>
								</div>
								
							</div>
                                       
							<div class ="row">
								<div class ="col-md-4">
									<div class="form-group">
										<label>Destination From </label>
											<select id ="inputDestinationFrom" class ="form-control">
												<option selected>---Select---</option>
												<option>Singapore</option>
											</select>
									</div>
								</div>
								
								<div class ="col-md-4">
									<div class="form-group">
										<label>Destion To </label>
											<select id ="inputDestionTo" class ="form-control">
												<option selected>---Select---</option>
												<option>Merak</option>
											</select>
									</div>
								</div>
								
							</div>
								
								
							<button type="submit" class="btn btn-info btn-fill pull-right">Save</button>
							<div class="clearfix"></div>
						  		
                            </form>
                          </div>
						  
                       </div>
                    </div>
			    </div>
